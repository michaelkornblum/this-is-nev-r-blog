
	<div class="form-group">
		<label class="col-form-label" for="field-label">Label</label>
		<input
			class="form-control"
			type="text"
			name="label"
			id="field-label"
			value="<%= field.config.label %>"
			placeholder="ex: Categories"
			required
		/>
    </div>

    <div class="form-group">
		<label class="col-form-label" for="field-options">Options</label>
		<input
			class="form-control"
			type="text"
			name="options"
			id="field-options"
			value="<%= field.config.options %>"
			placeholder="ex: Option1, Option2, Option3"
			required
		/>
    </div>
